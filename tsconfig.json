{
    "compilerOptions": {
        "allowJs": false,
        "baseUrl": "./front",
        "checkJs": false,
        "module": "ES2020",
        "moduleResolution": "node",
        "target": "ES6",
        "jsx": "react",
        "skipLibCheck": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "types": [],
        "noImplicitAny": true,
        "allowSyntheticDefaultImports": true,
        "outDir": "./build"
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*",
        "**/*.json",
        "./build/**/*"
    ]
}